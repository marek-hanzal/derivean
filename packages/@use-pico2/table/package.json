{
	"version": "1.0.11",
	"name": "@use-pico2/table",
	"description": "Table & List support stuff.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/use-pico2/pico"
	},
	"sideEffects": false,
	"type": "module",
	"main": "src/index.ts",
	"module": "src/index.ts",
	"types": "src/index.ts",
	"files": [
		"src"
	],
	"scripts": {
		"build": "concurrently npm:build:*",
		"build:esbuild": "node esbuild.mjs",
		"build:types": "tsc --outDir lib",
		"lint": "eslint src",
		"cleanup": "concurrently npm:cleanup:*",
		"cleanup:sdk": "del-cli src/sdk/**",
		"cleanup:lib": "del-cli .turbo lib tsconfig.tsbuildinfo src/**/*.{d.ts,d.ts.map,js,js.map}"
	},
	"dependencies": {
		"@tabler/icons-react": "^2.32.0",
		"@use-pico2/i18n": "^1.0.0",
		"@use-pico2/pagination": "^1.0.0",
		"@use-pico2/query": "^1.0.0",
		"@use-pico2/schema": "^1.0.0",
		"@use-pico2/selection": "^1.0.0",
		"@use-pico2/source": "^1.0.0",
		"@use-pico2/ui": "^1.0.0",
		"@use-pico2/utils": "^1.0.0",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@use-pico2/esbuild": "^1.0.8",
		"@use-pico2/eslint-config-eslint": "^1.0.10",
		"@use-pico2/tsconfig": "^1.0.10",
		"@types/node": "^20.4.9",
		"@types/react": "^18.2.20",
		"concurrently": "^8.2.0",
		"del-cli": "^5.0.0",
		"eslint": "^8.45.0",
		"typescript": "^5.2.2"
	},
	"bugs": {
		"url": "https://github.com/use-pico2/pico/issues"
	},
	"homepage": "https://github.com/use-pico2/pico#readme"
}
