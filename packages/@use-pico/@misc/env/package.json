{
	"version": "3.0.0",
	"name": "@use-pico/env",
	"description": "Next.js env helper stuff.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/use-pico/pico.git"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"sideEffects": false,
