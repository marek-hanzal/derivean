		"@use-pico/eslint-config-eslint": "^3.0.0",
		"@use-pico/tsconfig": "^3.0.0",
		"@types/node": "^20.10.1",
		"concurrently": "^8.2.0",
		"del-cli": "^5.1.0",
		"eslint": "^8.54.0",
		"typescript": "^5.3.2"
	},
	"bugs": {
		"url": "https://github.com/use-pico/pico/issues"
